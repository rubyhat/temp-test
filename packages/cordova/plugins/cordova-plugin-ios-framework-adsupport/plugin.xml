<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0" id="cordova-plugin-ios-framework-adsupport" version="1.0.0">
    <name>iOS AdSupport Framework</name>
    <description>Add AdSupport.framework to your xcode project generated by cordova</description>
    <license>Apache License, Version 2.0</license>
    <keywords>ionic,cordova,phonegap,ios,xcode,frameworks,binary,additions</keywords>
    <repo>https://github.com/tbetous/cordova-plugin-ios-framework-adsupport.git</repo>
    <issue>https://github.com/tbetous/cordova-plugin-ios-framework-adsupport/issues</issue>
    <!-- ios only -->
    <platform name="ios">
        <js-module src="www/plugin.js" name="plugin">
            <runs/>
            <clobbers target="iOSFrameworks" />
        </js-module>
        <config-file target="config.xml" parent="/*">
            <feature name="iOSFrameworks">
                <param name="ios-package" value="iOSFrameworks" onload="true" />
            </feature>
        </config-file>
        <header-file src="src/ios/iOSFrameworks.h" />
        <source-file src="src/ios/iOSFrameworks.m" />
        <framework src="AdSupport.framework" />
    </platform>
</plugin>
