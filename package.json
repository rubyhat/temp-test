{
    "name": "morda",
    "version": "2.16.31",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev": "PROXY=true node server.js",
        "dev-win": "set PROXY=true && node server.js",
        "dev-win-miotaxi": "set PROXY=true && set PORT=3030 && node server.js",
        "dev-win-compas": "set PROXY=true && set PORT=3040 && node server.js",
        "dev-saas": "PROXY=true PORT=3001 node server.js",
        "dev-saas-win": "set PROXY=true && set PORT=3001 && node server.js",
        "dev-internal": "INTERNAL_IP=$(internal-ip --ipv4) node server.js",
        "dev-analyze": "ANALYZE=true npm run dev",
        "dev:cordova-atlas": "CORDOVA=true CORDOVA_PARTNER_ID=atlas npm run dev",
        "dev:cordova-atlas-win": "set CORDOVA=true && set CORDOVA_PARTNER_ID=atlas && npm run dev-win",
        "dev:cordova-jaguar": "CORDOVA=true CORDOVA_PARTNER_ID=jaguar npm run dev",
        "build": "next build",
        "build-cordova": "CORDOVA=true NODE_OPTIONS=--openssl-legacy-provider next build && CORDOVA=true next export -o packages/cordova/www",
        "build-cordova-win": "set CORDOVA=true && next build && set CORDOVA=true && next export -o packages/cordova/www",
        "build-proxy": "PROXY=true next build",
        "build-proxy-win": "set PROXY=true && next build",
        "build-analyze": "ANALYZE=true npm run build",
        "build-sourcemaps": "SOURCEMAPS=true npm run build",
        "start": "NODE_ENV=production node server.js",
        "start-win": "set NODE_ENV=production && node server.js",
        "yo:component": "yo ./build-utils/generator/component",
        "typecheck": "tsc --noEmit",
        "storybook": "start-storybook -p 6006 --ci -s public",
        "start-storybook": "node ./scripts/storybook",
        "build-storybook": "build-storybook",
        "deploy": "gcloud app deploy --quiet",
        "deploy-storybook": "gcloud app deploy storybook-app.yaml",
        "fetch-openapi": "node swagger/bin/openapi-fetch-schema.js",
        "build-openapi": "node swagger/bin/openapi-generate.js",
        "bootstrap": "npm install && npm run build-openapi && localazy download",
        "lint": "eslint */**/*.{ts,tsx,js,json} --cache",
        "pre-commit": "lint-staged",
        "release": "npm run typecheck && release-it",
        "saas": "node saas/prepare.js",
        "postinstall": "patch-package"
    },
    "repository": {
        "type": "git",
        "url": "git+https://gitlab.com/atlasbus/morda.git"
